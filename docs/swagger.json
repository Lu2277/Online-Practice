{
  "swagger": "2.0",
  "info": {
    "description": "基于Gin、Gorm框架的在线练习系统",
    "title": "在线练习系统",
    "contact": {
      "name": "LL",
      "url": "http://www.swagger.io/support",
      "email": "support@swagger.io"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version": "1.0"
  },
  "host": "localhost:8000",
  "paths": {
    "/login": {
      "post": {
        "tags": [
          "公共接口"
        ],
        "summary": "用户登录",
        "parameters": [
          {
            "type": "string",
            "description": "用户名称username",
            "name": "username",
            "in": "formData"
          },
          {
            "type": "string",
            "description": "用户密码password",
            "name": "password",
            "in": "formData"
          }
        ],
        "responses": {
          "200": {
            "description": "{\"code\":200,\"data\":{},\"msg\":\"ok\"}",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    },
    "/problem-add": {
      "post": {
        "tags": [
          "管理员私有接口"
        ],
        "summary": "创建问题",
        "parameters": [
          {
            "type": "string",
            "description": "authorization",
            "name": "authorization",
            "in": "header",
            "required": true
          },
          {
            "type": "string",
            "description": "title",
            "name": "title",
            "in": "formData",
            "required": true
          },
          {
            "type": "string",
            "description": "content",
            "name": "content",
            "in": "formData",
            "required": true
          },
          {
            "type": "array",
            "description": "test_cases",
            "name": "test_cases",
            "in": "formData",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "{\"code\":200,\"data\":{},\"msg\":\"ok\"}",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    },
    "/problem-list": {
      "get": {
        "tags": [
          "公共接口"
        ],
        "summary": "问题列表",
        "parameters": [
          {
            "type": "integer",
            "description": "page",
            "name": "page",
            "in": "query"
          },
          {
            "type": "integer",
            "description": "size",
            "name": "size",
            "in": "query"
          },
          {
            "type": "string",
            "description": "keyword",
            "name": "keyword",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "{\"code\":200,\"data\":{},\"msg\":\"ok\"}",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    },
    "/rank-list": {
      "get": {
        "tags": [
          "公共接口"
        ],
        "summary": "用户排行榜",
        "responses": {
          "200": {
            "description": "{\"code\":200,\"data\":{},\"msg\":\"ok\"}",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    },
    "/register": {
      "post": {
        "tags": [
          "公共接口"
        ],
        "summary": "用户注册",
        "parameters": [
          {
            "type": "string",
            "description": "邮箱",
            "name": "email",
            "in": "formData",
            "required": true
          },
          {
            "type": "string",
            "description": "验证码",
            "name": "code",
            "in": "formData",
            "required": true
          },
          {
            "type": "string",
            "description": "名字",
            "name": "name",
            "in": "formData",
            "required": true
          },
          {
            "type": "string",
            "description": "密码",
            "name": "password",
            "in": "formData",
            "required": true
          },
          {
            "type": "string",
            "description": "手机号",
            "name": "phone",
            "in": "formData"
          }
        ],
        "responses": {
          "200": {
            "description": "{\"code\":200,\"data\":{},\"msg\":\"ok\"}",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    },
    "/send-code": {
      "post": {
        "tags": [
          "公共接口"
        ],
        "summary": "发送验证码",
        "parameters": [
          {
            "type": "string",
            "description": "email",
            "name": "email",
            "in": "formData",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "{\"code\":200,\"data\":{},\"msg\":\"ok\"}",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    },
    "/submit-list": {
      "get": {
        "tags": [
          "公共接口"
        ],
        "summary": "提交列表",
        "parameters": [
          {
            "type": "integer",
            "description": "page",
            "name": "page",
            "in": "query"
          },
          {
            "type": "integer",
            "description": "size",
            "name": "size",
            "in": "query"
          },
          {
            "type": "string",
            "description": "problem_identity",
            "name": "problem_identity",
            "in": "query"
          },
          {
            "type": "string",
            "description": "user_identity",
            "name": "user_identity",
            "in": "query"
          },
          {
            "type": "integer",
            "description": "status",
            "name": "status",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "{\"code\":200,\"data\":{},\"msg\":\"ok\"}",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    },
    "/user-list": {
      "get": {
        "tags": [
          "公共接口"
        ],
        "summary": "用户详情",
        "parameters": [
          {
            "type": "string",
            "description": "用户标识identity",
            "name": "identity",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "{\"code\":200,\"data\":{},\"msg\":\"ok\"}",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}